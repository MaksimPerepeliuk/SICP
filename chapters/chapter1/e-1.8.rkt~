#lang racket
(define (qbrt-iter guess x)
  (if (qbrt-good-enough? guess x)
      guess
      (qbrt-iter (qbrt-improve guess x)
                 x)))

(define (qbrt-improve guess x)
  (/ (+ (/ x
           (square guess))
        (* 2 guess))
     3))

(define (qbrt-good-enough? guess x)
  (< (abs (- (* guess guess guess) x)) 0.001))

(define (my-qbrt x)
  (qbrt-iter 1.0 x))

(my-qbrt 27)